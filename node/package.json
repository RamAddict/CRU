{
  "name": "chaincode",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build::transpile": "tsc --project tsconfig.json",
    "build::create_dir": "sudo mkdir -p ../vars/chaincode/mycc/node",
    "build::copy": "sudo cp -rf ./dist/* ../vars/chaincode/mycc/node",
    "build::copy_package_json": "sudo cp ./package.json ../vars/chaincode/mycc/node",
    "build": "run-s build::transpile build::create_dir build::copy build::copy_package_json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.19",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "fabric-shim": "^2.2.2",
    "pnpm": "^6.25.0"
  }
}
